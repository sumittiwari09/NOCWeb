@using NewZapures_V2.Models
@model List<Partial.ApesSetCommission>
@{
    ViewBag.Title = "DTH Commission Of Partner";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-sm-12">
        <div class="page-title-box">
            <div class="float-right">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item">Zapures</li>
                    <li class="breadcrumb-item">@ViewBag.Title </li>

                </ol>
            </div>
            <h4 class="page-title">@ViewBag.Title</h4>
        </div><!--end page-title-box-->
    </div><!--end col-->
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header">
                <h4 class="mt-0 header-title float-left">@ViewBag.Title</h4>
            </div>
            <div class="card-body">

                <form class="form-parsley" action="#" autocomplete="off">
                    @*<table id="datatable-buttons" class="table table-striped table-bordered dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">*@

                    <table id="datatable-buttons" class="table table-striped " style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                        <thead>
                            <tr>
                                <th colspan="11" style="
    text-align: center;
    font-size: large;
    background: chocolate;
    /* font-size: 184%; */
    font-weight: bold;
">DTH COMMISSION</th>
                            </tr>
                            <tr style=" text-align: center; background: bisque; font-size: large; font-weight: bold; ">
                                <th colspan="3"></th>

                                <th colspan="2">Retailer</th>
                                <th colspan="2">Distributer</th>
                                <th colspan="2">Stockist</th>
                                <th colspan="2">WhiteLabel</th>
                            </tr>
                            <tr style="
    background: burlywood;
    font-size: initial;
">
                                <th>#No</th>
                                <th>Transaction From</th>
                                <th>Transaction To</th>
                                <th>Commission % of Customer Fees</th>
                                <th>Commission Amount </th>
                                <th>Commission % of Customer Fees</th>
                                <th>Commission Amount </th>
                                <th>Commission % of Customer Fees</th>
                                <th>Commission Amount </th>
                                <th>Commission % of Customer Fees</th>
                                <th>Commission Amount </th>
                            </tr>
                        </thead>
                        <tbody>
                            @{
                                var i = 1;
                                foreach (var item in Model)
                                {
                                                        <tr style="
    background-color: cadetblue;
    font-size: initial;
    color: antiquewhite;
">
                                                            @*<button type="button" class="btn btn-secondary btn-circle">
            <i class="fab fa-twitter"></i>
        </button>*@
                                                            <td>
                                                                @i
                                                            </td>
                                                            <td>
                                                                @item.TransactionAmountFrom
                                                            </td>
                                                            <td>
                                                                @item.TransactionAmountTo
                                                            </td>
                                                            <td>
                                                                @item.Retailer_Commission
                                                            </td>
                                                            <td>
                                                                @item.Retailer
                                                            </td>
                                                            <td>
                                                                @item.Distributer_Commission
                                                            </td>
                                                            <td>
                                                                @item.Distributer
                                                            </td>
                                                            <td>
                                                                @item.Stockist_Commission
                                                            </td>
                                                            <td>
                                                                @item.Stockist
                                                            </td>
                                                            <td>
                                                                @item.Whitelabel_Commission
                                                            </td>
                                                            <td>
                                                                @item.Whitelabel
                                                            </td>
                                                        </tr>
                                    i++;
                                }
                            }
                        </tbody>

                    </table>
                </form>
            </div>
        </div>
    </div>
</div>

