@using NewZapures_V2.Models

@{
    ViewBag.Title = "Report";
    Layout = "~/Views/Shared/_Layout.cshtml";

    var commissionDistribution = ZapurseCommonlist.GetCommissionDistribution();
    var Transection = ZapurseCommonlist.GetTransections();

}

@section styles{  @*@Styles.Render("~/bundles/datatable");*@
<link href="~/plugins/datatables/dataTables.bootstrap4.min.css" rel="stylesheet" type="text/css" />
<link href="~/plugins/datatables/buttons.bootstrap4.min.css" rel="stylesheet" type="text/css" />
<link href="~/plugins/datatables/responsive.bootstrap4.min.css" rel="stylesheet" type="text/css" />
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
<style>

    .DivDetails {
        background: #0d6efd;
        text-align: center;
        color: white;
        padding: 10px;
        border-radius: 10px;
    }
</style>
}
@*@if (CurrentSessions.View == "True")
    {*@
<div class="row">
    <div class="col-sm-12">
        <div class="page-title-box">
            <div class="float-right">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item">Zapures</li>
                    <li class="breadcrumb-item">Transection</li>
                    <li class="breadcrumb-item active">Report</li>
                </ol>
            </div>
            <h4 class="page-title">@ViewBag.Title</h4>
        </div><!--end page-title-box-->
    </div><!--end col-->
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header">
                <h4 class="mt-0 header-title float-left">Report</h4>


                @*<button onclick="location.href='@Url.Action("Create")';return false;" class="btn btn-gradient-primary float-right waves-effect waves-light"><i class="mdi mdi-plus-circle-outline mr-2"></i> @ViewBag.Create</button>*@
            </div>
            <div class="card-body">
                <form class="form-parsley" action="#" autocomplete="off">
                    @*<table id="datatable-buttons" class="table table-striped table-bordered dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">*@
                    <div class="DivDetails">Transection Data</div>
                    <div style="overflow-x:scroll">
                        <table class="table table-striped " style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                            <thead>
                                <tr>
                                    <td>Id</td>
                                    <td>TransectionId</td>
                                    <td>UserType</td>
                                    <td>PartyId</td>
                                    <td>ParentsId</td>
                                    <td>RefrenceId</td>
                                    <td>ServicetypeId</td>
                                    <td>OperatorId</td>
                                    <td>ServiceProviderId</td>
                                    <td>TransectionAmount</td>
                                    <td>CustomerFeeType</td>
                                    <td>CustomerFee</td>
                                    <td>ValueOfCustomerFee</td>
                                    <td>Commission</td>
                                    <td>Calc_Commission</td>
                                    <td>State</td>
                                    <td>IGST</td>
                                    <td>Calc_IGST</td>
                                    <td>CGST</td>
                                    <td>Calc_CGST</td>
                                    <td>SGST</td>
                                    <td>Calc_SGST</td>
                                    <td>TDS</td>
                                    <td>Calc_TDS</td>
                                    <td>Profit</td>
                                    <td>Purchase_Price</td>
                                    <td>MobileNo</td>
                                    <td>Latitude</td>
                                    <td>Longitude</td>
                                    <td>IPAddress</td>
                                    <td>Card</td>
                                    <td>Bank</td>
                                    <td>Account No</td>
                                    <td>IFSC</td>
                                    <td>Branch</td>
                                </tr>
                            </thead>
                            <tbody>
                                @{
                                    if (Transection != null)
                                    {
                                        foreach (var item in Transection)
                                        {
                                            <tr>
                                                <td>@item.Id</td>
                                                <td>@item.TransectionId</td>
                                                <td>@item.UserType</td>
                                                <td>@item.PartyId</td>
                                                <td>@item.ParentsId</td>
                                                <td>@item.RefrenceId</td>
                                                <td>@item.ServicetypeId</td>
                                                <td>@item.OperatorId</td>
                                                <td>@item.ServiceProviderId</td>
                                                <td>@item.TransectionAmount</td>
                                                <td>@item.CustomerFeeType</td>
                                                <td>@item.CustomerFee</td>
                                                <td>@item.ValueOfCustomerFee</td>
                                                <td>@item.Commission</td>
                                                <td>@item.Calc_Commission</td>
                                                <td>@item.State</td>
                                                <td>@item.IGST</td>
                                                <td>@item.Calc_IGST</td>
                                                <td>@item.CGST</td>
                                                <td>@item.Calc_CGST</td>
                                                <td>@item.SGST</td>
                                                <td>@item.Calc_SGST</td>
                                                <td>@item.TDS</td>
                                                <td>@item.Calc_TDS</td>
                                                <td>@item.Profit</td>
                                                <td>@item.Purchase_Price</td>
                                                <td>@item.MobileNo</td>
                                                <td>@item.Latitude</td>
                                                <td>@item.Longitude</td>
                                                <td>@item.IPAddress</td>
                                                <td>@item.Bank</td>
                                                <td>@item.AccountNo</td>
                                                <td>@item.IFSC</td>
                                                <td>@item.Branch</td>

                                            </tr>

                                        }
                                    }
                                    else
                                    {
                                        <tr><td colspan="29">No Data Available</td></tr>
                                    }
                                }
                            </tbody>



                        </table>
                    </div>


                </form>
            </div>
            <div class="card-body">
                <form class="form-parsley" action="#" autocomplete="off">

                    <div class="DivDetails">Commission Distribution Data</div>
                    <table class="table table-striped " style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                        <thead>

                            <tr>

                                <td>Id</td>
                                <td>TransectionId</td>
                                <td>PartyId</td>
                                <td>PartyCommission</td>
                                <td>Calc_PartyCommission</td>
                                <td>State</td>
                                <td>IGST</td>
                                <td>Calc_IGST</td>
                                <td>CGST</td>
                                <td>Calc_CGST</td>
                                <td>SGST</td>
                                <td>Calc_SGST</td>
                                <td>TDS</td>
                                <td>Calc_TDS</td>
                                <td>Profit</td>
                                <td>SoldPrice</td>
                                <td>PurchasePrice</td>


                            </tr>
                        </thead>
                        <tbody>
                            @{
                                if (commissionDistribution != null)
                                {
                                    foreach (var item in commissionDistribution)
                                    {
                                        <tr>
                                            <td>@item.Id</td>
                                            <td>@item.TransectionId</td>
                                            <td>@item.PartyId</td>
                                            <td>@item.PartyCommission</td>
                                            <td>@item.Calc_PartyCommission</td>
                                            <td>@item.State</td>
                                            <td>@item.IGST</td>
                                            <td>@item.Calc_IGST</td>
                                            <td>@item.CGST</td>
                                            <td>@item.Calc_CGST</td>
                                            <td>@item.SGST</td>
                                            <td>@item.Calc_SGST</td>
                                            <td>@item.TDS</td>
                                            <td>@item.Calc_TDS</td>
                                            <td>@item.Profit</td>
                                            <td>@item.SoldPrice</td>
                                            <td>@item.PurchasePrice</td>
                                        </tr>

                                    }
                                }
                                else
                                {
                                    <tr><td colspan="29">No Data Available</td></tr>
                                }
                            }
                        </tbody>
                    </table>

                </form>
            </div>
        </div>
    </div>
</div>
@*}
    else
    {
        <h2 class="page-title text-danger">You do not have permission, Please contact admin!</h2>
    }*@

